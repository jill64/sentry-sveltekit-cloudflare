{
  "name": "@jill64/sentry-sveltekit-cloudflare",
  "version": "0.4.32",
  "description": "Unofficial Sentry Integration for SvelteKit Cloudflare Adapter",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/sentry-sveltekit-cloudflare/issues",
  "homepage": "https://github.com/jill64/sentry-sveltekit-cloudflare#readme",
  "files": [
    "dist"
  ],
  "keywords": [
    "svelte",
    "worker",
    "edge"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/sentry-sveltekit-cloudflare.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "cd packages/site && pnpm dev",
    "build:pages": "pnpm build:lib && pnpm build:site && npx publint",
    "build:lib": "cd packages/lib && pnpm build",
    "build:site": "cd packages/site && pnpm build",
    "preview": "pnpm build:lib && cd packages/site && pnpm preview",
    "prepack": "pnpm build:lib",
    "test:e2e": "npx playwright install && playwright test",
    "lint": "npx prettier --check . --plugin prettier-plugin-svelte . && npx eslint && pnpm check",
    "format": "npx prettier --write . --plugin prettier-plugin-svelte .",
    "check": "pnpm build:lib && export PUBLIC_SENTRY_DSN= && cd packages/site && pnpm check"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^1.0.0"
  },
  "devDependencies": {
    "@playwright/test": "1.39.0",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@typescript-eslint/parser": "6.8.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-svelte": "2.34.0",
    "prettier-plugin-svelte": "3.0.3",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@sentry/core": "7.74.1",
    "@sentry/svelte": "7.74.1",
    "@sentry/utils": "7.74.1",
    "@sveltejs/kit": "1.26.0",
    "esm-env": "1.0.0",
    "toucan-js": "3.3.0"
  }
}
