{
  "name": "@jill64/sentry-sveltekit-cloudflare",
  "version": "1.0.0",
  "description": "♟️ Unofficial Sentry Integration for SvelteKit Cloudflare Adapter",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/sentry-sveltekit-cloudflare/issues",
  "homepage": "https://github.com/jill64/sentry-sveltekit-cloudflare#readme",
  "files": [
    "dist",
    "!*.test.*",
    "!*.spec.*"
  ],
  "keywords": [
    "cloudflare",
    "edge",
    "sentry",
    "svelte",
    "worker"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/sentry-sveltekit-cloudflare.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "cd demo && npm run dev",
    "build:pages": "npm run build:lib && npm run build:demo && npx publint",
    "build:lib": "tsc && npx tsx scripts/build.ts",
    "build:demo": "cd demo && npm run build",
    "preview": "npm run build:lib && cd demo && npm run preview",
    "prepack": "npm run build:lib",
    "test": "playwright test",
    "lint": "npm run build:lib && npm run lint:lib && npm run lint:demo",
    "lint:lib": "npx eslint .",
    "lint:demo": "cd demo && npm run lint",
    "format": "npm run format:lib && npm run format:demo",
    "format:lib": "npx prettier --write .",
    "format:demo": "cd demo && npm run format"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^1.0.0"
  },
  "devDependencies": {
    "@playwright/test": "1.39.0",
    "@sentry/core": "7.80.0",
    "@sentry/svelte": "7.80.0",
    "@sentry/types": "7.80.0",
    "@sentry/utils": "7.80.0",
    "@sveltejs/kit": "1.27.4",
    "@types/node": "20.9.0",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "esbuild": "0.19.5",
    "eslint-plugin-deprecation": "2.0.0",
    "toucan-js": "3.3.1",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "esm-env": "1.0.0"
  }
}
