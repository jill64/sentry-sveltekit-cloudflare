{
  "name": "@jill64/sentry-sveltekit-cloudflare",
  "version": "0.4.15",
  "description": "Unofficial Sentry Integration for SvelteKit Cloudflare Adapter",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/sentry-sveltekit-cloudflare/issues",
  "homepage": "https://github.com/jill64/sentry-sveltekit-cloudflare#readme",
  "files": [
    "dist"
  ],
  "keywords": [
    "svelte",
    "worker",
    "edge"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/sentry-sveltekit-cloudflare.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm build:lib && pnpm build:site && publint",
    "build:lib": "cd packages/lib && pnpm build",
    "build:site": "cd packages/site && pnpm build",
    "lint": "prettier --check . --plugin prettier-plugin-svelte . && eslint .",
    "format": "prettier --write . --plugin prettier-plugin-svelte .",
    "check": "cd packages/site && pnpm check"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^1.0.0"
  },
  "devDependencies": {
    "@playwright/test": "1.38.1",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-svelte": "2.33.2",
    "prettier": "3.0.3",
    "prettier-plugin-svelte": "3.0.3",
    "publint": "0.2.3",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@sentry/core": "^7.68.0",
    "@sentry/svelte": "^7.68.0",
    "@sentry/utils": "^7.68.0",
    "@sveltejs/kit": "^1.24.1",
    "esm-env": "^1.0.0",
    "toucan-js": "^3.2.3"
  }
}
