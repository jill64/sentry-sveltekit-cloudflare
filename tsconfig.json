{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "declarationDir": "dist",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "skipLibCheck": true,
    "paths": {
      "@sentry-sveltekit/*": [
        "./src/sentry-javascript/packages/sveltekit/src/*"
      ]
    }
  },
  "include": ["src"],
  "exclude": ["src/sentry-javascript/**"]
}
